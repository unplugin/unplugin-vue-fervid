{
  "name": "unplugin-vue-fervid",
  "version": "0.0.3",
  "packageManager": "pnpm@9.15.4",
  "description": "Transform Vue 3 SFC to JavaScript written in Rust.",
  "type": "module",
  "keywords": [
    "vue",
    "sfc",
    "unplugin",
    "vite",
    "webpack",
    "rollup",
    "esbuild"
  ],
  "license": "MIT",
  "homepage": "https://github.com/unplugin/unplugin-vue-fervid#readme",
  "bugs": {
    "url": "https://github.com/unplugin/unplugin-vue-fervid/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unplugin/unplugin-vue-fervid.git"
  },
  "files": [
    "*.d.ts",
    "dist"
  ],
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./vite": {
      "require": "./dist/vite.cjs",
      "import": "./dist/vite.js"
    },
    "./webpack": {
      "require": "./dist/webpack.cjs",
      "import": "./dist/webpack.js"
    },
    "./rspack": {
      "require": "./dist/rspack.cjs",
      "import": "./dist/rspack.js"
    },
    "./rollup": {
      "require": "./dist/rollup.cjs",
      "import": "./dist/rollup.js"
    },
    "./rolldown": {
      "require": "./dist/rolldown.cjs",
      "import": "./dist/rolldown.js"
    },
    "./esbuild": {
      "require": "./dist/esbuild.cjs",
      "import": "./dist/esbuild.js"
    },
    "./api": {
      "require": "./dist/api.cjs",
      "import": "./dist/api.js"
    },
    "./*": "./*"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "scripts": {
    "test": "vitest",
    "build": "tsup",
    "dev": "tsup --watch",
    "release": "bumpp",
    "lint": "eslint --max-warnings 0 .",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm run build"
  },
  "peerDependencies": {
    "vue": "^3.2.25"
  },
  "dependencies": {
    "@fervid/napi": "^0.3.1",
    "@vue/reactivity": "^3.5.13",
    "debug": "^4.4.0",
    "unplugin": "^1.16.1",
    "vite": "^6.0.11"
  },
  "devDependencies": {
    "@babel/types": "^7.26.7",
    "@jridgewell/gen-mapping": "^0.3.8",
    "@jridgewell/trace-mapping": "^0.3.25",
    "@sxzz/eslint-config": "^4.6.0",
    "@sxzz/prettier-config": "^2.1.1",
    "@sxzz/test-utils": "^0.4.0",
    "@types/debug": "^4.1.12",
    "@types/node": "^22.13.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitest/ui": "^3.0.4",
    "bumpp": "^9.11.1",
    "cross-env": "^7.0.3",
    "esbuild": "^0.24.2",
    "eslint": "^9.19.0",
    "fast-glob": "^3.3.3",
    "rimraf": "^6.0.1",
    "rollup": "^4.34.0",
    "rollup-plugin-esbuild": "^6.1.1",
    "slash": "^5.1.0",
    "source-map-js": "^1.2.1",
    "tsup": "^8.3.6",
    "typescript": "^5.7.3",
    "unplugin-unused": "^0.3.0",
    "vitest": "^3.0.4",
    "webpack": "^5.97.1"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "prettier": "@sxzz/prettier-config"
}
