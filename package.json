{
  "name": "unplugin-vue-fervid",
  "version": "0.0.1",
  "packageManager": "pnpm@8.14.0",
  "description": "Transform Vue 3 SFC to JavaScript. written in Rust",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./vite": {
      "require": "./dist/vite.cjs",
      "import": "./dist/vite.js"
    },
    "./webpack": {
      "require": "./dist/webpack.cjs",
      "import": "./dist/webpack.js"
    },
    "./rollup": {
      "require": "./dist/rollup.cjs",
      "import": "./dist/rollup.js"
    },
    "./esbuild": {
      "require": "./dist/esbuild.cjs",
      "import": "./dist/esbuild.js"
    },
    "./api": {
      "require": "./dist/api.cjs",
      "import": "./dist/api.js"
    },
    "./*": "./*"
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "scripts": {
    "test": "vitest",
    "build": "tsup",
    "dev": "tsup --watch",
    "release": "bumpp",
    "lint": "eslint --max-warnings 0 .",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm run build"
  },
  "peerDependenciesMeta": {
    "vite": {
      "optional": true
    }
  },
  "dependencies": {
    "debug": "^4.3.4",
    "unplugin": "~1.6.0",
    "vite": "^5.0.11"
  },
  "devDependencies": {
    "@babel/types": "^7.23.6",
    "@jridgewell/gen-mapping": "^0.3.3",
    "@jridgewell/trace-mapping": "^0.3.20",
    "@types/debug": "^4.1.12",
    "@types/node": "^20.10.6",
    "@vitejs/plugin-vue": "5.0.2",
    "@vitest/ui": "1.1.3",
    "bumpp": "^9.2.1",
    "esbuild": "0.19.11",
    "eslint": "^8.56.0",
    "fast-glob": "^3.3.2",
    "rimraf": "^5.0.5",
    "rollup": "^4.9.4",
    "rollup-plugin-esbuild": "^6.1.0",
    "slash": "^5.1.0",
    "source-map-js": "^1.0.2",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "1.1.3",
    "vue": "^3.4.5",
    "webpack": "^5.89.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "vue",
    "sfc",
    "unplugin",
    "vite",
    "webpack",
    "rollup",
    "esbuild",
    "rust"
  ],
  "homepage": "https://github.com/unplugin/unplugin-vue-fervid#readme",
  "bugs": {
    "url": "https://github.com/unplugin/unplugin-vue-fervid/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unplugin/unplugin-vue-fervid.git"
  },
  "files": [
    "*.d.ts",
    "dist"
  ],
  "author": "",
  "license": "MIT"
}
